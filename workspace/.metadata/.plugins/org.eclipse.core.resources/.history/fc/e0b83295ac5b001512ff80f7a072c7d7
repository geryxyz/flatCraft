package flatcraft;

import java.awt.event.KeyEvent;

public class Key {
	public static final int LEFT = KeyEvent.VK_LEFT;
	public static final int RIGHT = KeyEvent.VK_LEFT;
	public static final int UP = KeyEvent.VK_LEFT;
	public static final int DOWN = KeyEvent.VK_LEFT;
	public static final int SPACE = KeyEvent.VK_LEFT;
	public static final int ENTER = KeyEvent.VK_LEFT;
}
